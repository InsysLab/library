<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="549.0000999999975" prefWidth="612.0" spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controller.SearchCheckoutRecordController">
  <children>
    <HBox alignment="CENTER_LEFT" prefHeight="60.0" prefWidth="200.0" spacing="10.0">
      <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Search Checkout Records">
          <font>
            <Font name="System Bold" size="18.0" />
          </font>
          <HBox.margin>
            <Insets left="30.0" fx:id="x1" />
          </HBox.margin>
        </Text>
      </children>
    </HBox>
    <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0" spacing="10.0" VBox.margin="$x1">
      <children>
        <Label text="Member ID" />
        <TextField id="tfSearchText" fx:id="tfSearchID" prefWidth="80.0" promptText="Enter ID" />
        <TextField fx:id="tfMemberName" editable="false" opacity="1.0" prefWidth="200.0" />
        <Button mnemonicParsing="false" onAction="#onSearchBtnAction" prefWidth="70.0" text="Search" />
      </children>
    </HBox>
    <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0">
      <children>
        <Label fx:id="lblSearchStatus" />
      </children>
    </HBox>
    <HBox fx:id="hbSearchResult" prefHeight="330.0" prefWidth="612.0" />
  </children>
  <padding>
    <Insets left="5.0" right="5.0" />
  </padding>
</VBox>
